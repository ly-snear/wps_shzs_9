<UserControl x:Class="CalligraphyAssistantMain.Controls.BlankStudentControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CalligraphyAssistantMain.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Width="134" Height="80" Margin="3,0,3,5">
        <Rectangle x:Name="backRect" StrokeThickness="2" StrokeDashArray="4" Stroke="#9E9E9E"/>
        <Rectangle x:Name="focusRect" StrokeThickness="2" StrokeDashArray="4" Stroke="#FEA356" Visibility="Collapsed" />
    </Grid> 
</UserControl>
